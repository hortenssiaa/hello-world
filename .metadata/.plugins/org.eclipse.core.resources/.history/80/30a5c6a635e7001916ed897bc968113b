		//3-4
		Scanner sc = new Scanner(System.in);
		System.out.print("두 수 입력(0제외):");
		int num1 = sc.nextInt();
		int num2 = sc.nextInt();
		int div=0;
		int result=0;
		
		if(num1 != 0 && num2 != 0){
			if(num1 > num2) {
				div = num1;
				if(div%num2 ==0)
					result =0;
				else
					result = div/num2;
			} else if(num1 < num2) {
				div = num2;
				if(div%num1 ==0)
					result=0;
				else
					result = div/num1;
			}
		}
		System.out.println(num1+"와 "최대 공약수는 ");
		
		
		
	}

}
