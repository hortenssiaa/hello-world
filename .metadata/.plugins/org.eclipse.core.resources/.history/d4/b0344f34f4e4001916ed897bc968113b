import java.util.Scanner;

public class Ch3_Practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.print("임의의 정수를 입력하시오>>");
		int x = sc.nextInt();
		
		for(int i=x; i>0; i--){
			for(int j=x; j>0; j--)
				System.out.print("*");
			System.out.println();
		}
	}

}
