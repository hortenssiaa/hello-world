package Chapter5_practice1_6;

public class Printer {
	private String model;
	private String manufacture;
	private String interfaceType;
	private int numOfPrints=0; //인쇄 매수
	private int numOfSheets=50; //인쇄 종이 잔량
	
	public Printer(int n, String mo, String ma, String inter){
		numOfPrints=n;
		model = "BCP212";
		manufacture = "Kangnam";
		interfaceType = "USB";
		}
	protected void print(){
		System.out.println("Model: ");
		System.out.println("Number of copies: "+numOfPrints);
		System.out.println("Remains of Sheets: "+(numOfSheets-numOfPrints));
	}
}
