	public static void main(String args[]){
		
		//p145 3번문
		Scanner sc = new Scanner(System.in);
		int i[][] = new int[4][];
		i[0] = new int[4];
		i[1] = new int[1];
		i[2] = new int[1];
		i[3] = new int[4];
		
		for(int x=0; x<4; x++) {
			for(int y=0; y<i[x].length; y++){
				i[x][y] = sc.nextInt();
				System.out.print(i[x][y]+"\t");
			} System.out.println();
		}
	}
}
