package Chapter5_practice1_6;

public class Printer {
	//프린터는 인쇄 매수를 받아야 한다
	// 
	String model;
	String manufacture;
	String interfaceType;
	int numOfPrints=50; //인쇄 매수
	int numOfSheets=50; //인쇄 종이 잔량
	
	protected void print(){
		//print 호출하면 인쇄가 되는 거잖아
		//그니깐, numOfSheets--; + 종이잔량 알려주기
		//인쇄매수는 여기 말고 다른데에서 set
		System.out.println("인쇄 종이 잔량: ");
	}
}
