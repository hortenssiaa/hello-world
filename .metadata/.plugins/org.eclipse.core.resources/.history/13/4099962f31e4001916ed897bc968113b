package gaba_part3;

public class Ex3_12 {
	
	static int[] makeArray(){
		int[] tmp = new int[4];
		for(int i=0; i<4; i++)
			tmp[i] = i;
		return tmp;
	}
	
	public static void main(String args[]){
		int[] refer;
		refer = makeArray(); //refer 배열이 makeArray()원소를 참조함 
		for(int i=0; i<refer.length; i++)
			System.out.println(refer[i]);
	}
}
