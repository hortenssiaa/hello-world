import java.util.Scanner;

public class Ch3_Practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("임의의 정수를 입력하시오>>");
		int x=sc.nextInt();
		for(int i=0; i<x; i--){
			for(int j=0; j<x; j--)
				System.out.print("*");
			System.out.println();
		}
	}

}
