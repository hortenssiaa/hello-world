package ch5_examples;

class Person{
	int age; //default
	public String name;
	protected int height;
	private int weight;
	void setWeight(int weight){
		this.weight = weight;
	}
	void getWeight(){
		System.out.println(weight);
	}
}

public class Student extends Person{
	
	public static void main(String[] args) {
		Student s1 = new Student();
		s1.age = 25;
		s1.name = "손하경";
		s1.height=168;
		s1.setWeight(53);
	}

}
