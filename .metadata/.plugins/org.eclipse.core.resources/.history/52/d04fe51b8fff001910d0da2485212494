package Chapter5_practice1_6;

public class Printer {
	protected String model;
	protected String manufacture;
	protected String interfaceType;
	protected int numOfPrints=0; //인쇄 매수
	protected int numOfSheets=50; //인쇄 종이 잔량
	
	public Printer(int a){
		numOfPrints=a;}
	protected void print(){
		System.out.println("Model: ");
		System.out.println("Number of copies: "+numOfPrints);
		System.out.println("Remains of Sheets: "+(numOfSheets-numOfPrints));
	}
	protected void setPrinter(){
		model = "BCP212";
		manufacture = "Kangnam";
		interfaceType = "USB";
	}
}
