import java.util.Scanner;

public class study_part3 {
	public static void main(String args[]) {
		/*
		//ex3-1
		int sum=0;
		for(int i=1;i<=10;i++) {
			sum += i;
			if(i==10)
				System.out.print(i+"="+sum);
			else
				System.out.print(i+"+");
		}
		*/
		
		//ex3-2
		Scanner sc = new Scanner(System.in);
		int sum = 0;
		int count =0;
		while(sc.nextInt()!=0){
			sum += sc.nextInt();
			count++;
		}
		System.out.println(count+"개 입력한 수의 평균은 "+sum);
	}
}
